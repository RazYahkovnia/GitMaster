{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.preferences.quoteStyle": "single",
    "files.exclude": {
        "out": false,
        "node_modules": true
    },
    "search.exclude": {
        "out": true,
        "node_modules": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "Worktree"
    ]
}

